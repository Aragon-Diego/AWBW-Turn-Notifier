{
  "manifest_version": 3,
  "name": "AWBW Notifications",
  "description": "A google chrome extension that notifiers users when it's their turn on Advance Wars By Web.",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "host_permissions": [
    "*://awbw.amarriner.com/"
  ],
  "permissions": ["storage", "scripting", "activeTab", "cookies", "notifications", "alarms"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+U",
        "mac": "Command+U"
      }
    }
  }
}